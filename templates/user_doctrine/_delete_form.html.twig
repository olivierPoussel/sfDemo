<form method="post" action="{{ path('user_doctrine_delete', {'id': user_doctrine.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user_doctrine.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
